{
    "name": "yosmy/blackstone-payment-gateway",
    "description": "",
    "require": {
        "php": ">=7.2",
        "ext-mongodb": "*",
        "ext-json": "*",
        "yosmy/mongo": "@dev",
        "yosmy/error": "@dev",
        "yosmy/http": "@dev",
        "yosmy/log": "@dev",
        "yosmy/payment-gateway": "@dev"
    },
    "require-dev": {
        "symsonte/framework": "@dev",
        "symfony/debug": "^4.4",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Yosmy\\Payment\\Gateway\\Blackstone\\": [
                "src/card",
                "src/charge",
                "src/communication",
                "src/customer",
                "src/exception"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yosmy\\Payment\\Gateway\\Blackstone\\Play\\": [
                "play/setup",
                "play/src/card",
                "play/src/charge",
                "play/src/customer"
            ],
            "Yosmy\\Payment\\Gateway\\Blackstone\\Test\\": [
                "test/setup",
                "test/src/customer"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://php.yosmy.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
